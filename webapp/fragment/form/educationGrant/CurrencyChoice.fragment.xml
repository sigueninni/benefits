<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <SelectDialog
        id="currencySelectDialog"
        noDataText="{i18n>noCurrencyFound}"
        title="{i18n>selectCurrency}"
        search="onSearchCurrencySelectDialog"
        confirm="onConfirmCurrencySelectDialogPress"
        items="{currencyModel>/items}"
        rememberSelections="false"
    >
        <ObjectListItem title="{currencyModel>Id}">
            <ObjectAttribute text="{currencyModel>Txt}" />
        </ObjectListItem>
    </SelectDialog>
</core:FragmentDefinition>