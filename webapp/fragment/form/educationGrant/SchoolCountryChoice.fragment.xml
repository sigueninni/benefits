<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <SelectDialog
        id="schoolCountrySelectDialog"
        noDataText="{i18n>noSchoolCountryFound}"
        title="{i18n>selectSchoolCountry}"
        search="onSearchSchoolCountrySelectDialog"
        confirm="onConfirmSchoolCountrySelectDialogPress"
        items="{schoolCountryModel>/items}"
        rememberSelections="false"
    >
        <ObjectListItem title="{schoolCountryModel>Id}">
            <ObjectAttribute text="{schoolCountryModel>Txt}" />
        </ObjectListItem>
    </SelectDialog>
</core:FragmentDefinition>